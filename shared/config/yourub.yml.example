yourub_defaults: &yourub_defaults
  developer_key: '[developer_key]'
  youtube_api_service_name: 'youtube'
  youtube_api_version: 'v3'
  application_name: 'yourub'
  application_version: '0.1'
  log_level: WARN

development:
  <<: *yourub_defaults

test:
  <<: *yourub_defaults

staging:
  <<: *yourub_defaults

production:
  <<: *yourub_defaults